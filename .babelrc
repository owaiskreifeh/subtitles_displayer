{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": 6
        }
      }
    ],
    "@babel/preset-flow"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import",
    "transform-es3-member-expression-literals",
    "transform-es3-property-literals",
    "@babel/plugin-transform-regenerator"
  ]
}
